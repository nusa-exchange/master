keychain:
  applogic:
    algorithm: RS256
    value: <%= @jwt_public_key %>
  peatio:
    algorithm: RS256
    value: <%= @jwt_public_key %>
    
scopes:
  read_users:
    mandatory_signers:
      - applogic
    permitted_signers:
      - applogic
  otp_sign:
    mandatory_signers:
      - applogic
    permitted_signers:
      - applogic
  send_phones:
    mandatory_signers:
      - peatio
    permitted_signers:
      - peatio
